server {
    listen 80 default_server;
    root /container/web;
    location / {
#        index index.php;
#        autoindex on;
        try_files $uri $uri/ =404;
    }

    location /key/ {
        try_files $uri $uri/ =404;
        alias /key/;
    }

    location ~ \.php$ {
        include snippets/fastcgi-php.conf;
        fastcgi_pass unix:/var/run/php/php7.0-fpm.sock;
    }
}